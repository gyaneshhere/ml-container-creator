{
  "name": "generator-ml-container-creator",
  "version": "1.0.0",
  "description": "Generator for SageMaker AI BYOC paradigm for predictive inference use-cases.",
  "type": "module",
  "main": "generators/app/index.js",
  "keywords": [
    "yeoman-generator",
    "sagemaker",
    "byoc",
    "aws",
    "machine-learning",
    "docker"
  ],
  "scripts": {
    "test": "VALIDATE_ENV_VARS=false mocha test/**/*.test.js --reporter test/reporters/progress-reporter.cjs",
    "test:ci": "VALIDATE_ENV_VARS=false mocha test/**/*.test.js --reporter spec",
    "test:verbose": "VALIDATE_ENV_VARS=false mocha test/**/*.test.js --reporter spec",
    "test:minimal": "VALIDATE_ENV_VARS=false mocha test/**/*.test.js --reporter test/reporters/minimal-reporter.cjs",
    "test:watch": "VALIDATE_ENV_VARS=false mocha test/**/*.test.js --watch --reporter test/reporters/progress-reporter.cjs",
    "test:coverage": "VALIDATE_ENV_VARS=false nyc mocha test/**/*.test.js",
    "test:unit": "VALIDATE_ENV_VARS=false mocha test/unit/**/*.test.js test/helpers/**/*.test.js --reporter spec",
    "test:integration": "VALIDATE_ENV_VARS=false mocha test/input-parsing-and-generation/**/*.test.js --reporter spec",
    "test:property": "VALIDATE_ENV_VARS=false mocha test/property/**/*.test.js --reporter spec",
    "test:all": "npm run test:ci",
    "test:perf": "VALIDATE_ENV_VARS=false mocha test/**/*.test.js --reporter json > test-results.json && node scripts/analyze-test-performance.js",
    "security-audit": "npm audit --audit-level=high",
    "security-fix": "npm audit fix",
    "pretest": "npm run security-audit",
    "lint": "eslint generators/**/*.js test/**/*.js",
    "lint:fix": "eslint generators/**/*.js test/**/*.js --fix",
    "start": "yo ml-container-creator",
    "dev": "npm link && yo ml-container-creator",
    "clean": "rm -rf test-output-*",
    "validate": "npm run lint && npm run test:all"
  },
  "dependencies": {
    "@microsoft/eslint-formatter-sarif": "^3.1.0",
    "ajv": "^8.12.0",
    "license-report": "^6.8.0",
    "lodash": "^4.17.21",
    "sbom": "^0.0.0",
    "yeoman-generator": "^7.5.1",
    "yosay": "^3.0.0"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "fast-check": "^4.5.2",
    "mocha": "^10.2.0",
    "npm-force-resolutions": "^0.0.10",
    "nyc": "^15.1.0",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^10.1.1"
  },
  "engines": {
    "node": ">=24.11.1",
    "npm": ">=11.6.2"
  },
  "overrides": {
    "yosay": {
      "lodash": "^4.17.21",
      "trim-newlines": "^5.0.0"
    },
    "yeoman-generator": {
      "lodash": "^4.17.21",
      "semver-regex": "^4.0.5",
      "semver": "^7.5.4"
    },
    "yeoman-doctor": {
      "semver-regex": "^3.1.3"
    },
    "yo": {
      "qs": "^6.14.1",
      "request": "2.88.2"
    },
    "insight": {
      "request": "2.88.2"
    },
    "cross-spawn": "^7.0.3",
    "diff": "^5.2.2",
    "dot-prop": "^8.0.2",
    "form-data": "^2.5.4",
    "lodash": "^4.17.21",
    "semver": "^7.5.4",
    "semver-regex": "^3.1.3",
    "trim-newlines": "^5.0.0",
    "got": "^11.8.5",
    "tough-cookie": "^4.1.3",
    "tmp": "^0.2.3",
    "qs": "^6.14.1",
    "request": "2.88.2",
    "tar": "^7.5.3"
  },
  "resolutions": {
    "semver-regex": "4.0.5"
  },
  "author": "Dan Ferguson",
  "license": "Apache-2.0",
  "private": true,
  "peerDependencies": {
    "yo": "^1.4.2"
  }
}
